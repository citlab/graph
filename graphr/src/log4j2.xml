<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%5p [%d{HH:mm:ss.SSS}] (%F:%L) - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
<!-- 	
		<Logger name="com.foo.Bar" level="trace">
			<AppenderRef ref="Console"/>
		</Logger>
 -->		
		<Root level="trace">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>
